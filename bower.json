{
  "name": "forca-de-vendas",
  "version": "0.0.0",
  "authors": [
    "Luiz Cunha <felipe.silvacunha@gmail.com>",
    "Marcel Mossman"
  ],
  "description": "Sistema de Vendas",
  "keywords": [
    "php",
    "js",
    "web",
    "git"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "~3.3.4",
    "angularjs": "~1.3.15",
    "angular-route": "~1.3.15",
    "angular-resource": "~1.4.0",
    "angular-bootstrap": "~0.13.0",
    "jquery.inputmask": "~3.1.63",
    "select2": "~4.0.0",
    "ng-flow": "~2"
  },
  "resolutions": {
    "angular": "1.4.0"
  },
  "exportsOverride": {
    "angular*": {
      ".": [
        "*.js",
        "*.gzip",
        "*.map"
      ]
    },
    "bootstrap": {
      "js": "dist/js/[^n]*.js",
      "css": "dist/css/*",
      "fonts": "dist/fonts/*"
    },
    "jquery": {
      ".": [
        "dist/*.js",
        "dist/*.map"
      ]
    },
    "jquery-ui": {
      "js": "jquery-ui.min.js",
      "css": "themes/cupertino/jquery-ui.min.css",
      "css/images": "themes/cupertino/images/*"
    }
  }
}
